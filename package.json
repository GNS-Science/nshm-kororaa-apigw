{
  "name": "nshm-kororaa-api",
  "version": "0.2.0",
  "description": "A stitched graphql API for Kororaa UI.",
  "dependencies": {
    "@graphql-tools/delegate": "^11.1.2",
    "@graphql-tools/load": "^8.1.4",
    "@graphql-tools/schema": "^10.0.27",
    "@graphql-tools/stitch": "^10.1.2",
    "@graphql-tools/stitching-directives": "^4.0.4",
    "@graphql-tools/url-loader": "^9.0.2",
    "@graphql-tools/utils": "^10.10.1",
    "@graphql-tools/wrap": "^11.0.4",
    "@vendia/serverless-express": "^4.12.6",
    "@whatwg-node/fetch": "^0.10.12",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.2",
    "graphql-yoga": "^4.0.5",
    "pino": "^10.1.0",
    "serverless-http": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@graphql-tools/mock": "^9.1.1",
    "@serverless/eslint-config": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.1.0",
    "get-graphql-schema": "^2.1.2",
    "git-list-updated": "^1.2.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "serverless": "^4.23.0",
    "serverless-offline": "^14.4.0",
    "serverless-plugin-warmup": "^8.3.0"
  },
  "main": "handler.mjs",
  "scripts": {
    "start": "node server.js",
    "test": "jest test",
    "deploy": "serverless deploy --stage ${STAGE}",
    "sls_doctor": "serverless doctor",
    "lint": "eslint  --ignore-pattern .gitignore .",
    "lint:updated": "pipe-git-updated --ext=js -- eslint --ignore-path .gitignore",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettify:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier --write"
  },
  "author": "",
  "license": "AGPL3",
  "eslintConfig": {
    "extends": [
      "@serverless/eslint-config/node"
    ],
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "packageManager": "yarn@4.11.0"
}
